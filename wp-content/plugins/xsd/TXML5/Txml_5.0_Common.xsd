			<xs:enumeration value="RJ">
				<xs:annotation>
					<xs:documentation>Rack, clothing hanger </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RL">
				<xs:annotation>
					<xs:documentation>Reel (A spool on which thread, wire, film, etc, is wound). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="RO">
				<xs:annotation>
					<xs:documentation>Roll (A package of goods wound into a ball or cylinder). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SA">
				<xs:annotation>
					<xs:documentation>Sack (A large bag of coarse cloth for storing or moving grain, foodstuffs, etc).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SH">
				<xs:annotation>
					<xs:documentation>Sachet (A small sealed envelope). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SL">
				<xs:annotation>
					<xs:documentation>Slipsheet (A firm sheet of plastic, cardboard or other material which may be hooked or attached to a forklift or other transportation equipment. The slipsheet is used to pull products stacked on top of it). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SO">
				<xs:annotation>
					<xs:documentation>Spool (Any cylindrical piece on which something is wound). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="STL">
				<xs:annotation>
					<xs:documentation>Stick (A container for dispensing solid substances, e.g. glue, deodorant). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SW">
				<xs:annotation>
					<xs:documentation>Shrinkwrapped (A transport unit load whose contents are secured together through the use of transparent or semi-transparent filmwrap). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SX">
				<xs:annotation>
					<xs:documentation>Set TAE Tablet (A small rectangular package of aluminium foil or paper, e.g. a tablet of chocolate). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TB">
				<xs:annotation>
					<xs:documentation>Tub (A round, open, flat bottomed wooden container). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TEV">
				<xs:annotation>
					<xs:documentation>Tamper evident package (A type of package giving easy or immediate recognition that the package has been tampered with after it has been sealed). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="THE">
				<xs:annotation>
					<xs:documentation>Three pack (A package containing three products). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TRE">
				<xs:annotation>
					<xs:documentation>Trolley (A low cart for the transportation and storage of groceries, milk, etc). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TTE">
				<xs:annotation>
					<xs:documentation>Tube, standing (A screw-topped pliable cylinder capable of standing and suitable for holding pastes or semi-liquids, e.g. a tube of toothpaste). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TU">
				<xs:annotation>
					<xs:documentation>Tube (A screw-topped pliable cylinder suitable for holding pastes or semi-liquids, e.g. a tube of toothpaste). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TV">
				<xs:annotation>
					<xs:documentation>Tube, with nozzle </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TWE">
				<xs:annotation>
					<xs:documentation>Two pack (A package containing two products). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="TY">
				<xs:annotation>
					<xs:documentation>Tank, cylindrical (A cylindrical container suitable for the storage or transportation of liquids or gases). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UUE">
				<xs:annotation>
					<xs:documentation>Tube net (A plastic or textile tube suitable for carrying loose products, e.g. fruit). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="UVQ">
				<xs:annotation>
					<xs:documentation>Wrapped in plastic sheet (An indication that the items have been wrapped in plastic sheet). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VI">
				<xs:annotation>
					<xs:documentation>Vial Small (usually glass) container. (E.g. for a liquid medicine or perfume). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="VP">
				<xs:annotation>
					<xs:documentation>Vacuumpacked (A package from which all air has been removed in order to keep the contents fresh). </xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X10">
				<xs:annotation>
					<xs:documentation>Tub with lid (A small, usually plastic, container with a lid for food, e.g. ice cream).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X11">
				<xs:annotation>
					<xs:documentation>Banded package (A package with bands, usually metal or nylon, round it to hold the products together).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X12">
				<xs:annotation>
					<xs:documentation>Cardboard package (with grip holes for bottles Cardboard package with a number of holes. Each hole is to be gripped tightly around the neck of a bottle).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X14">
				<xs:annotation>
					<xs:documentation>Card A flat package (usually made of fibreboard from/to which the product is often hung or attached).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X3">
				<xs:annotation>
					<xs:documentation>Standard stack of stones.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="X9">
				<xs:annotation>
					<xs:documentation>Pallet 100 X 110 cm (Pallet with dimensions 100 X 110 cm).</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="ItemProcessingType">
		<xs:sequence>
			<xs:element name="Successor" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates to the seller if the buyer allows them to substitute the requested article with successor articles.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Substitution" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates to the seller if the buyer allows them to substitute the requested article with alternative articles with equivalent functionality.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExchangePart" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates to the seller if the buyer allows them to susbstitute the requested article with refurbished articles.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ItemType">
		<xs:sequence>
			<xs:element name="Number">
				<xs:annotation>
					<xs:documentation>The item position number.

1.  This number **must** be a positive integer and be greater than the position number of the previous item (if any).

2.  This number **must** be unique over all item instances.

3.  It **must not** be reset when a new package starts.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:minExclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="DeliveredQuantity" type="QuantityType">
				<xs:annotation>
					<xs:documentation>The quantity and unit of measurement of the item that is delivered with this shipment.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductId" type="ProductIdChoiceType">
				<xs:annotation>
					<xs:documentation>The identifiers of the item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProductName" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The name of the item.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="256"/>
						<xs:minLength value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Reference" type="ReferenceLineType" maxOccurs="99">
				<xs:annotation>
					<xs:documentation>A reference to another document.

1.  The seller **must** always add a reference to the buyer's order with the qualifier `BuyerOrderNumber`. Its value **must** be the `DocumentNumber` of the buyer's order.

2.  The seller **must** always add a reference to the buyer's order with the qualifier `SellerOrderNumber`. Its value **must** be the order number the seller has assigned to the buyer's order in their ERP system.

3.  The seller **can** add more than one reference with the qualifier `SellerOrderNumber`, for example, if the order was split internally in their ERP system.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Measurements" type="MeasurementsType" minOccurs="0" maxOccurs="10">
				<xs:annotation>
					<xs:documentation>A measurement of the item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExternalTrade" type="ExternalTrade" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Information that is relevant for the export of the item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ExternalDocument" type="ExternalDocumentType" minOccurs="0" maxOccurs="9">
				<xs:annotation>
					<xs:documentation>An external resource with information about the item.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AdditionalParameters" type="AdditionalParametersType" minOccurs="0" maxOccurs="30">
				<xs:annotation>
					<xs:documentation>Additional information related to the processing of the item.

The seller **must** only use these qualifiers:

1.  `StorageLocation`
2.  `MaterialGroup`</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FreeText" type="FreeTextValueType" minOccurs="0" maxOccurs="50">
				<xs:annotation>
					<xs:documentation>Arbitrary information that applies to the item.

Free text **must** be agreed between buyer and seller, otherwise the buyer will ignore it.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="MeasurementsType">
		<xs:sequence>
			<xs:element name="Qualifier" type="DimensionCodeList">
				<xs:annotation>
					<xs:documentation>The type of measurement.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="UnitNetWeight"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Unit" type="UnitCodeList">
				<xs:annotation>
					<xs:documentation>The unit of measurement.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="Kilogram"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Value">
				<xs:annotation>
					<xs:documentation>The actual measurement.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="50"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:fractionDigits value="5"/>
						<xs:totalDigits value="16"/>
						<xs:minInclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="OrderProcessingInstructionsType">
		<xs:sequence>
			<xs:element name="DispatchMode" type="DispatchModeCodeList">
				<xs:annotation>
					<xs:documentation>The dispatch mode of the order.

Every dispatch mode offered by the seller has a corresponding delivery window, for example, 3 to 5 days for dispatch mode `Road`.

The seller **must** reject the order if the buyer has sent an unsupported dispatch mode.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="Road-Express"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="RequestedDeliveryDate" type="DateType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The date when the buyer wants to have the goods delivered.

This date is only a refinement of the delivery window defined by the chosen dispatch mode.

The date **must** lie inside this delivery window, otherwise the seller **may** ignore it.

**Example**: for the dispatch mode `Road` the seller specifies a delivery window of 3 to 5 days. If the order date is `2024-01-09`, then the earliest possible delivery date would be `2024-01-12`and the latest possible delivery date would be `2024-01-16` (based on working days from Monday to Friday). The requested delivery date therefore must lie in the window between the earliest and latest delivery date.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="WarehouseReference" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The reference to a location from which the goods should be shipped.

1.  Warehouse references **must** be:
    
    1.  mutually agreed between buyer and seller or
    2.  taken from `Line/DeliveryOptions` in an availability response. 

2.  If this reference is not known to the seller, they will ignore it.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="Warehouse Münster"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="25"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Backlog" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates to the seller if the buyer wants them to back-order the remaining quantities of all articles with insufficient quantity and deliver them at a later date.

The seller **can** override this setting per dispatch mode, but they must specify in their membership profile whether they:

1.  always back-order articles,
2.  never back-order articles or
3.  back-order articles according to the buyer's setting.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="true"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CompleteDelivery" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates to the seller if they should only create the order if all articles are available in the requested quantities.

This is of higher priority than the backlog setting. That is, if complete delivery is requested, the seller **must** check the requested quantities against the actually available stock at hand.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="false"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="CompleteShipment" type="xs:boolean">
				<xs:annotation>
					<xs:documentation>Indicates to the seller if they **must** deliver the whole order in one shipment.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="false"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PartyType1">
		<xs:sequence>
			<xs:element name="Number">
				<xs:annotation>
					<xs:documentation>The identifier of the party involved in the transaction.

This identifier is used by the TecCom Order system for routing messages.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="DE928378"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1"/>
						<xs:maxLength value="14"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="AdditionalPartyNumber" type="AdditionalPartyNumberType" minOccurs="0" maxOccurs="8">
				<xs:annotation>
					<xs:documentation>An additional identifier of the party involved in the transaction.

This identifier is not used by the TecCom Order system for routing messages.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Address" type="AddressType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The address information of the party.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Contact" type="ContactType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The contact information of the party.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PartyType2">
		<xs:sequence>
			<xs:choice>
				<xs:sequence>
					<xs:element name="Number">
						<xs:annotation>
							<xs:documentation>The identifier of the delivery party involved in the transaction.

This identifier can be used by the TecCom Order system for routing messages.</xs:documentation>
							<xs:appinfo>
								<altova:exampleValues>
									<altova:example value="DE24231"/>
								</altova:exampleValues>
							</xs:appinfo>
						</xs:annotation>
						<xs:simpleType>
							<xs:restriction base="xs:string">
								<xs:minLength value="0"/>
								<xs:maxLength value="14"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="AdditionalPartyNumber" type="AdditionalPartyNumberType" minOccurs="0" maxOccurs="8">
						<xs:annotation>
							<xs:documentation>An additional identifier of the delivery party involved in the transaction.

This identifier is not used by the TecCom Order system for routing messages.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Address" type="AddressType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The address information of the delivery party.

1.  This address is for information purposes only.

2.  The seller **must not** send the goods to an address sent by the buyer in the delivery party.

3.  The delivery party address **must** always be retrieved from the seller ERP by means of the `Number`.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Contact" type="ContactType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The contact information of the delivery party.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
				<xs:sequence>
					<xs:element name="AdditionalPartyNumber" type="AdditionalPartyNumberType" minOccurs="0" maxOccurs="8">
						<xs:annotation>
							<xs:documentation>An additional identifier of the delivery party involved in the transaction.

This identifier is not used by the TecCom Order system for routing messages.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Address" type="AddressType">
						<xs:annotation>
							<xs:documentation>The address information of the delivery party.

1.  This address is for information purposes only.

2.  The seller **must not** send the goods to an address sent by the buyer in the delivery party.

3.  The delivery party address **must** always be retrieved from the seller ERP by means of the `Number`.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Contact" type="ContactType" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The contact information of the delivery party.</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:choice>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PkgIdType">
		<xs:sequence>
			<xs:element name="System">
				<xs:annotation>
					<xs:documentation>The numbering system of the code by which the package is identified on the label.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="SSCC"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="Supplier">
							<xs:annotation>
								<xs:documentation>Seller-specific code.</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="SSCC">
							<xs:annotation>
								<xs:documentation>Serial shipping container code assigned by GS1.</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
						<xs:enumeration value="EAN">
							<xs:annotation>
								<xs:documentation>International article number.</xs:documentation>
							</xs:annotation>
						</xs:enumeration>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="Number">
				<xs:annotation>
					<xs:documentation>The actual identification code.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="340123451234567895"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="0"/>
						<xs:maxLength value="35"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PkgInfoType">
		<xs:sequence>
			<xs:element name="PackageKind" type="PackageKindTypeList">
				<xs:annotation>
					<xs:documentation>The type of the packaging material.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="CT"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Description" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The description of the packaging material.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="70"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="TotalPackages" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The number of directly subordinate inner or intermediate packages contained in this package.</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:int">
						<xs:totalDigits value="7"/>
						<xs:minExclusive value="0"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PriceType">
		<xs:sequence>
			<xs:element name="Category" type="AdditionalPriceCategoryCodeList">
				<xs:annotation>
					<xs:documentation>The type of the price.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="Bulk"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name="Quantity">
				<xs:annotation>
					<xs:documentation>The quantity to which this price applies.</xs:documentation>
					<xs:appinfo>
						<altova:exampleValues>
							<altova:example value="100"/>
						</altova:exampleValues>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:minInclusive value="0"/>
						<xs:totalDigits value="9"/>
						<xs:fractionDigits value="5"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="UoM" type="UoMCodeList">
