				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Contract">
				<xs:annotation>
					<xs:documentation>Price that was contractually agreed.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Distributor">
				<xs:annotation>
					<xs:documentation>Price of the distributor.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Quotation">
				<xs:annotation>
					<xs:documentation>Price quoted in a preceding offer.</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="AdditionalProductIdQualifierCodeList">
		<xs:restriction base="xs:string">
