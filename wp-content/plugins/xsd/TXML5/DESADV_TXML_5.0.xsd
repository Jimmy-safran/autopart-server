<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:altova="http://www.altova.com/xml-schema-extensions" elementFormDefault="qualified" attributeFormDefault="unqualified">
    <xs:include schemaLocation="Txml_5.0_Common.xsd"/>
    <xs:element name="DesAdv">
        <xs:annotation>
            <xs:documentation>The despatch advice is used by the seller to inform the buyer about the delivery of ordered articles.</xs:documentation>
            <xs:appinfo>
                <history>
                    <version>v1.0</version>
                    <changedBy>RMA</changedBy>
                    <changedOn>20210311</changedOn>
                    <change>Upgraded to TXML 5.0 Common 1.7; added namespace; added enumeration lists</change>
                </history>
                <history>
                    <version>v1.1</version>
                    <changedBy>RMA</changedBy>
                    <changedOn>20210803</changedOn>
                    <change>Upgraded to TXML 5.0 Common 1.8; schema updated</change>
                </history>
                <history>
                    <version>v1.2</version>
                    <changedBy>RHA</changedBy>
                    <changedOn>20210827</changedOn>
                    <change>Upgraded to TXML 5.0 Common 1.9; removed promotion from Reference</change>
                </history>
                <history>
                    <version>v1.3</version>
                    <changedBy>RMA</changedBy>
                    <changedOn>20211005</changedOn>
                    <change>Upgraded to TXML 5.0 Common 1.10</change>
                </history>
                <history>
                    <version>v1.3</version>
                    <changedBy>MDR</changedBy>
                    <changedOn>20220927</changedOn>
                    <change>Upgraded to TXML 5.0 Common 1.11</change>
                </history>
                <history>
                    <version>v1.4</version>
                    <changedBy>CD</changedBy>
                    <changedOn>20240124</changedOn>
                    <change>Removed schema version from filename</change>
                </history>
                <history>
                    <version>v1.5</version>
                    <changedBy>CD</changedBy>
                    <changedOn>20240124</changedOn>
                    <change>Changed shipping marks; defined handling instruction codes; extended hazardous goods</change>
                </history>
                <history>
                    <version>v1.6</version>
                    <changedBy>TR</changedBy>
                    <changedOn>20240521</changedOn>
                    <change>Updated schema-internal documentation</change>
                </history>
            </xs:appinfo>
        </xs:annotation>
        <xs:complexType>
            <xs:sequence>
                <xs:element name="Head">
                    <xs:annotation>
                        <xs:documentation>Information about the whole despatch advice.</xs:documentation>
                    </xs:annotation>
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="DocumentNumber">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">The identifier of the despatch advice.

This **must** be unique over all despatch advices the seller sends (that is, no two despatch advices may share the same identifier).</xs:documentation>
                                    <xs:appinfo>
                                        <altova:exampleValues>
                                            <altova:example value="DESADV-20240104-140512-13"/>
                                        </altova:exampleValues>
                                    </xs:appinfo>
                                </xs:annotation>
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:minLength value="0"/>
                                        <xs:maxLength value="35"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="IssueDate" type="xs:dateTime">
                                <xs:annotation>
                                    <xs:documentation xml:lang="en">The specific point in time when the despatch advice was created.

It **must** include the time zone (which is the number of hours and minutes the given date and time is ahead or behind UTC time).</xs:documentation>
                                    <xs:appinfo>
                                        <altova:exampleValues>
                                            <altova:example value="2024-01-04T14:05:15+01:00"/>
                                        </altova:exampleValues>
                                    </xs:appinfo>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="DispatchDate">
                                <xs:annotation>
                                    <xs:documentation>The date and, possibly, time when the shipment was or will be despatched.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="Qualifier">
                                            <xs:annotation>
                                                <xs:documentation>How the date is to be interpreted.</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="At"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="DateTypeQualifierCodeList">
                                                    <xs:enumeration value="Before">
                                                        <xs:annotation>
                                                            <xs:documentation>Latest possible date.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:enumeration>
                                                    <xs:enumeration value="At">
                                                        <xs:annotation>
                                                            <xs:documentation>Exact date.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:enumeration>
                                                    <xs:enumeration value="After">
                                                        <xs:annotation>
                                                            <xs:documentation>Earliest possible date.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:enumeration>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="Date" type="xs:date">
                                            <xs:annotation>
                                                <xs:documentation>The actual date of despatch.</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="2024-01-04"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="Time" type="xs:time" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>The actual time of despatch.

It **must** include the time zone (which is the number of hours and minutes the given date and time is ahead or behind UTC time).</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="14:05:15+01:00"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="DeliveryDate" type="DateType">
                                <xs:annotation>
                                    <xs:documentation>The date when the recipient will receive the shipment.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="Reference" type="ReferenceHeaderType" minOccurs="0" maxOccurs="9999">
                                <xs:annotation>
                                    <xs:documentation>A reference to another document.

1.  The seller **must** always add references to all of the buyer's orders contained in this shipment with the qualifier `BuyerOrderNumber`. Its values **must** be the `DocumentNumber`s of the buyer's orders.

2.  The seller **must** always add references to all of the buyer's orders contained in this shipment with the qualifier `SellerOrderNumber`. Its values **must** be the order numbers the seller has assigned to the buyer's orders in their ERP system.

3.  The seller **can** add more than one reference with the qualifier `SellerOrderNumber`, for example, if the order was split internally in their ERP system.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="TransportDetails" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>The transport details for the shipment.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="DispatchMode" type="DispatchModeCodeList" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>The dispatch mode the seller has used for the shipment.</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="Road-Express"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="Description" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Additional descriptive text.</xs:documentation>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:minLength value="0"/>
                                                    <xs:maxLength value="17"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="TransitDirection" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>The direction of travel for the shipment.</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="SupplierToBuyer"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="BuyerToSupplier">
                                                        <xs:annotation>
                                                            <xs:documentation>Shipment from the buyer back to the seller, for example, if the buyer returns unsold stock.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:enumeration>
                                                    <xs:enumeration value="SupplierToBuyer">
                                                        <xs:annotation>
                                                            <xs:documentation>Shipment from the seller to the buyer. The normal direction of travel for an order the buyer has placed with the seller.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:enumeration>
                                                    <xs:enumeration value="misc">
                                                        <xs:annotation>
                                                            <xs:documentation>Miscellaneous.</xs:documentation>
                                                        </xs:annotation>
                                                    </xs:enumeration>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="LicensePlate" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>The license plate or registration number of the carrier vehicle.</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="M-TA 9999"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:minLength value="0"/>
                                                    <xs:maxLength value="35"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="Shipment" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Information about the whole shipment.</xs:documentation>
                                </xs:annotation>
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="ShipmentNumber" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>The shipment number assigned to the shipment.</xs:documentation>
                                                <xs:appinfo>
                                                    <altova:exampleValues>
                                                        <altova:example value="234235"/>
                                                    </altova:exampleValues>
                                                </xs:appinfo>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:minLength value="0"/>
                                                    <xs:maxLength value="35"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="Info" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>Information about the outermost packages in the shipment.</xs:documentation>
                                            </xs:annotation>
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="TotalOuterPackages">
                                                        <xs:annotation>
                                                            <xs:documentation>The number of outermost packages in the shipment.

For example, if the shipment would consist of 40 cartons and 20 cartons are packaged on one pallet and 20 cartons on another pallet, then the 2 pallets are the outermost packages. 

On the other hand, if the shipment would consist of 2 individual cartons (that is, both have a shipping label), then the 2 cartons are the outermost packages.</xs:documentation>
                                                            <xs:appinfo>
                                                                <altova:exampleValues>
                                                                    <altova:example value="2"/>
                                                                </altova:exampleValues>
                                                            </xs:appinfo>
                                                        </xs:annotation>
                                                        <xs:simpleType>
                                                            <xs:restriction base="xs:int">
                                                                <xs:minExclusive value="0"/>
                                                                <xs:totalDigits value="7"/>
                                                            </xs:restriction>
                                                        </xs:simpleType>
                                                    </xs:element>
                                                    <xs:element name="OuterPackages" minOccurs="0" maxOccurs="9999">
                                                        <xs:annotation>
                                                            <xs:documentation>Detailed information about an outermost package.</xs:documentation>
                                                        </xs:annotation>
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="PackageKind">
                                                                    <xs:annotation>
                                                                        <xs:documentation>The type of packaging material.</xs:documentation>
                                                                        <xs:appinfo>
                                                                            <altova:exampleValues>
                                                                                <altova:example value="201"/>
                                                                            </altova:exampleValues>
                                                                        </xs:appinfo>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="PackageKindTypeList"/>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="Description" minOccurs="0">
                                                                    <xs:annotation>
                                                                        <xs:documentation>The human-readable description of the packaging material.</xs:documentation>
                                                                        <xs:appinfo>
                                                                            <altova:exampleValues>
                                                                                <altova:example value="Euro pallet"/>
                                                                            </altova:exampleValues>
                                                                        </xs:appinfo>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:maxLength value="70"/>
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="Amount">
                                                                    <xs:annotation>
                                                                        <xs:documentation>The number of packaging materials of this kind.</xs:documentation>
                                                                        <xs:appinfo>
                                                                            <altova:exampleValues>
                                                                                <altova:example value="2"/>
                                                                            </altova:exampleValues>
                                                                        </xs:appinfo>
                                                                    </xs:annotation>
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:int">
                                                                            <xs:minExclusive value="0"/>
                                                                            <xs:totalDigits value="7"/>
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="Measurements" type="MeasurementsType" minOccurs="0" maxOccurs="10">
                                            <xs:annotation>
                                                <xs:documentation>A measurement of the shipment.</xs:documentation>
                                            </xs:annotation>
                                        </xs:element>
                                        <xs:element name="TrackingLink" minOccurs="0">
                                            <xs:annotation>
                                                <xs:documentation>The hyperlink to an external resource (usually a web application on the internet) used to track the shipment.</xs:documentation>
                                            </xs:annotation>
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:maxLength value="2000"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="SellerParty" type="PartyType1">
                                <xs:annotation>
                                    <xs:documentation>Information about the seller.

The `Number` for the seller party is usually assigned by the buyer. It is the number under which the seller is known in the buyer's ERP system.

The seller **must** return their own address and contact information.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="BuyerParty" type="PartyType1">
                                <xs:annotation>
                                    <xs:documentation>Information about the buyer.

The `Number` for the buyer party is usually assigned by the seller. It is the number under which the buyer is known in the seller's ERP system.

1.  The seller **must** return the buyer's address and contact information stored in the seller ERP system for the buyer party number sent by the buyer.

2.  It is **not allowed** to copy these values from the order.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="DeliveryParty" type="PartyType2" minOccurs="0">
                                <xs:annotation>
                                    <xs:documentation>Information about the recipient of the shipment.

1.  The seller **must** return the recipient's address and contact information stored in their ERP system for the delivery party number sent by the buyer.

2.  It is **not allowed** to copy these values from the order.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="AdditionalParty" type="AdditionalPartyType" minOccurs="0" maxOccurs="10">
                                <xs:annotation>
                                    <xs:documentation>Information about another party involved in the shipment.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="ExternalDocument" type="ExternalDocumentType" minOccurs="0" maxOccurs="9">
                                <xs:annotation>
                                    <xs:documentation>An external resource that applies to the whole despatch advice.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="AdditionalParameters" type="AdditionalParametersType" minOccurs="0" maxOccurs="30">
                                <xs:annotation>
                                    <xs:documentation>An additional parameter that conveys specific information that applies to the whole document.

The seller **must** only use these qualifiers:

1.  `StorageLocation`</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                            <xs:element name="FreeText" type="FreeTextValueType" minOccurs="0" maxOccurs="50">
                                <xs:annotation>
                                    <xs:documentation>Arbitrary information that applies to the whole despatch advice.

Free text **must** be agreed between buyer and seller, otherwise the buyer will ignore it.</xs:documentation>
                                </xs:annotation>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:choice>
                    <xs:element name="Package" maxOccurs="9999">
                        <xs:annotation>
                            <xs:documentation>Information about a package contained in the shipment.

The seller **must** use this if they **can** provide the actual packaging hierarchy/structure.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                            <xs:sequence>
                                <xs:element name="Number">
                                    <xs:annotation>
                                        <xs:documentation>The package number.

1.  It **must** be unique over all package instances.

2.  It must start with `1` for the first package.

3.  It must increase by `1` for each additional package.

4.  This number is only used for identifying this package within the despatch advice, for example, for representing the package hierarchy/structure.</xs:documentation>
                                        <xs:appinfo>
                                            <altova:exampleValues>
                                                <altova:example value="1"/>
                                            </altova:exampleValues>
                                        </xs:appinfo>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:int">
                                            <xs:minExclusive value="0"/>
                                            <xs:totalDigits value="4"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="ParentId">
                                    <xs:annotation>
                                        <xs:documentation>The number of the parent package in which this package is contained.

This **must** be set to `0` If this package is at the outermost level and has no parent package.</xs:documentation>
                                        <xs:appinfo>
                                            <altova:exampleValues>
                                                <altova:example value="0"/>
                                            </altova:exampleValues>
                                        </xs:appinfo>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:int">
                                            <xs:totalDigits value="4"/>
                                            <xs:minInclusive value="0"/>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="Level" minOccurs="0">
                                    <xs:annotation>
                                        <xs:documentation>The package level.

This is a convenience information which states whether or not the package contains other packages and/or is contained in another package on the physical level.</xs:documentation>
                                        <xs:appinfo>
                                            <altova:exampleValues>
                                                <altova:example value="Outer"/>
                                            </altova:exampleValues>
                                        </xs:appinfo>
                                    </xs:annotation>
                                    <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                            <xs:enumeration value="Inner">
                                                <xs:annotation>
                                                    <xs:documentation>The package is located at the innermost level of the physical packaging hierarchy. It is contained in another package, but it does not contain other packages. For example, a carton with articles in it.</xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="Intermediate">
                                                <xs:annotation>
                                                    <xs:documentation>The package is located on an intermediate level of the physical packaging hierarchy. It is contained in another package and also contains other packages. For example, a container which contains cartons.</xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                            <xs:enumeration value="Outer">
                                                <xs:annotation>
                                                    <xs:documentation>The package is located on the outermost level of the physical packaging hierarchy. It is not contained in another package, but it contains other packages. For example, a pallet on which containers are packed.</xs:documentation>
                                                </xs:annotation>
                                            </xs:enumeration>
                                        </xs:restriction>
                                    </xs:simpleType>
                                </xs:element>
                                <xs:element name="Info" type="PkgInfoType" minOccurs="0">
                                    <xs:annotation>
                                        <xs:documentation>The detail packaging information about this package.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="Identification" type="PkgIdType" minOccurs="0" maxOccurs="3">
                                    <xs:annotation>
                                        <xs:documentation>An identifier of this package.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="Reference" type="ReferenceHeaderType" minOccurs="0">
                                    <xs:annotation>
                                        <xs:documentation>A reference to the packing list.

1.  The seller **must** use the qualifier `PackingListNumber`.

2.  The seller **must not** use this for any other kind of reference.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="Measurements" type="MeasurementsType" minOccurs="0" maxOccurs="10">
                                    <xs:annotation>
                                        <xs:documentation>A measurement of the package.</xs:documentation>
                                    </xs:annotation>
                                </xs:element>
                                <xs:element name="Handling" minOccurs="0" maxOccurs="10">
                                    <xs:annotation>
                                        <xs:documentation>A handling instruction for this package.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                        <xs:sequence>
                                            <xs:element name="HazardousMaterialClassCode" minOccurs="0">
                                                <xs:annotation>
                                                    <xs:documentation>The dangerous goods class for items in this package.</xs:documentation>
                                                </xs:annotation>
                                                <xs:simpleType>
                                                    <xs:restriction base="xs:string">
                                                        <xs:enumeration value="1">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 - Explosives.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="1.1">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 (Explosives) Division 1.1 - Mass Explosive Hazard.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="1.2">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 (Explosives) Division 1.2 - Projection Hazard.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="1.3">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 (Explosives) Division 1.3 - Fire and/or Minor Blast/Minor Projection Hazard.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="1.4">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 (Explosives) Division 1.4 - Minor Explosion Hazard.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="1.5">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 (Explosives) Division 1.5 - Very Insensitive With Mass Explosion Hazard.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="1.6">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 1 (Explosives) Division 1.6 - Extremely Insensitive; No Mass Explosion Hazard.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="2.1">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 2 (Gases) Division 2.1 - Flammable Gases.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="2.2">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 2 (Gases) Division 2.2 - Nonflammable Gases.</xs:documentation>
                                                            </xs:annotation>
                                                        </xs:enumeration>
                                                        <xs:enumeration value="2.3">
                                                            <xs:annotation>
                                                                <xs:documentation>Class 2 (Gases) Division 2.3 - Toxic Gases.</xs:documentation>
                                                            </xs:annotation>
